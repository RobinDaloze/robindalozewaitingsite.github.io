{
  "name": "robindaloze",
  "version": "1.0.0",
  "description": "test NPM",
  "scripts": {
    "browsersync:start": "browser-sync start --no-open --server \".\" --files \"*.html\" \"dist/\"",
    "build:css": "node-sass --output-style=compressed src/scss/main.scss dist/css/main.css ",
    "watch:css": "onchange src/scss -- npm run build:css",
    "del":"del-cli \"dist\"",
    "watch": "npm-run-all -p browsersync:start watch:css",
    "build":"npm-run-all --serial \"del\" \"build:**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinDaloze/robindaloze.github.io.git"
  },
  "author": "Robin Daloze",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/RobinDaloze/robindaloze.github.io/issues"
  },
  "homepage": "https://github.com/RobinDaloze/robindaloze.github.io#readme",
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "del-cli": "^1.1.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.1.0"
  },
  "dependencies": {}
}
